{
  "tagFormat": "v${version}",
  "repositoryUrl": "https://github.com/cracker2709/kubeclient-api-example",
  "plugins": [
    ["@semantic-release/commit-analyzer", {
        "preset": "angular",
        "releaseRules": [
        {"type": "build", "release": "patch"}
        ]
    }],
    ["@semantic-release/exec", {
      "prepareCmd": "mvn -q versions:set -DnewVersion=${nextRelease.version}"
    }],
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    ["@terrestris/maven-semantic-release", {
      "processAllModules": true,
      "updateSnapshotVersion": true
    }],
    ["@semantic-release/git", {
      "assets": ["pom.xml", "**/pom.xml", "CHANGELOG.md"],
      "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}[skip ci]"
    }],
    "@semantic-release/github"
  ],
  "branches": "main"
}